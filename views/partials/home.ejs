<form ng-submit="submit();" id="newsfeedForm" ng-controller="newsfeedController">
	<table bgcolor="white" background-color: none;>
	<tr>
		<td><img src="/images/updateStatus.png"></img><a href="" style="font-weight:bold" class="facepro_homepage_click loginpageText">Update Status</a>&nbsp;&nbsp;<img src="images/login_page_images/menu_divider.gif" height="25"></img></td>
		<td><img src="/images/createAlbum.png"></img>&nbsp;&nbsp;<a href="" style="font-weight:bold" class="facepro_homepage_click loginpageText">Add Photos/Video</a>&nbsp;&nbsp;<img src="images/login_page_images/menu_divider.gif" height="25"></img></td>
		<td>&nbsp;&nbsp;<a href="" style="font-weight:bold" class="facepro_homepage_click loginpageText">Create Photo Album</a></td>
	</tr>
		<tr><td colspan=3><textarea class="newsfeed_textarea" rows="5" cols="50" style="width:500px" name="newsfeed" ng-model="newsfeed" id="newsfeed"></textarea></td></tr>
		<tr><td colspan=3 align="right"><input type="submit" class="postButtonNewsfeed" value="Post" /></td></tr>
	</table>
	<table class="table" style="width:500px">
	<tr ng-repeat="eachItem in newsfeedData">
	<td><img src="/images/user.png" width="100" height="100"></img><a href="">{{eachItem.firstName}}</a> Posted "{{eachItem.newsfeed}}"<br/></td>
	</tr>
	</table>
	
</form>
			